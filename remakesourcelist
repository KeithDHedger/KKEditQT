#!/bin/bash

shopt -s nullglob

if [ -e ./toolspath ];then
	. ./toolspath
else
	./setuptools
fi

cd "KKEditQT/src"

rm moc_* &>/dev/null||true

$MOCPATH KKEditClass.h -o moc_KKEditClass.cpp
while read
	do
		outfile=${REPLY#./*}
		outfile=moc_${outfile%*.h}.cpp
		$MOCPATH $REPLY -o $outfile
	done < <(find -name "*QT*.h")

csrclst=$(echo "../src/*.c" "../src/*.cpp")
echo COMMONSRC = $csrclst > ../sources

exit 0
